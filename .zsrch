# Path to your oh-my-zsh installation.
export ZSH="/Users/jeffkern/.oh-my-zsh"

# Path setup for Google Cloud SDK
export PATH="/usr/lib/google-cloud-sdk/bin:$PATH"

# Set name of the theme to load
ZSH_THEME="robbyrussell"

# Which plugins would you like to load?
# Add plugins here
# plugins=(git)

source $ZSH/oh-my-zsh.sh

# User configuration
export NVM_DIR=~/.nvm

# Custom aliases
alias repos="cd ~/Documents/repos"

# Aliases for opening Google Chrome with specific URLs
alias email="open -a Google\ Chrome 'https://mail.google.com/mail/u/1/#inbox'"
alias github="open -a Google\ Chrome 'https://github.com/'"
alias messages="open -a Google\ Chrome 'https://messages.google.com/web/authentication'"

# Loading nvm and nvm bash_completion
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"
[ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"

# Aliasing python to python3
alias python=/usr/bin/python3
